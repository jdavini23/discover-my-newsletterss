    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.21.1",
    "uuid": "^11.0.3",
    "zustand": "^5.0.2"
